{
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "ruby-lsp": {
      "command": "ruby-lsp",
      "initializationOptions": {
        "experimentalFeaturesEnabled": false,
        "enabledFeatures": [
          "codeActions",
          "diagnostics",
          "documentHighlights",
          "documentLink",
          "documentSymbols",
          "foldingRanges",
          "formatting",
          "hover",
          "inlayHint",
          "onTypeFormatting",
          "selectionRanges",
          "semanticHighlighting",
          "completion"
        ],
        "formatter": "auto"
      },
      "filetypes": ["ruby"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "terraform",
    "ruby",
    "rb",
    "typescriptreact",
    "typescript",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc"
  ],
  "coc.preferences.willSaveHandlerTimeout": 5000
